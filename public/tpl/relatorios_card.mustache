{{#cards}}
    <div class="card radius no-select left theme-d2" style="width: 273px; max-width: 48%; margin-right: 1%; margin-left: 1%;padding-bottom: 5px;">
        <div class='col radius padding-left menu-li relative' style="background: linear-gradient(45deg, #00000030, transparent);padding: 10px 5px;">
            <span class='col s-font-small'>{{titulo}}</span>
            <span class='col overflow-hidden font-xxxlarge s-font-xxlarge align-right padding-right'>{{data}}</span>
            <span style="position: absolute;bottom: 10px;left: 15px;">
                <i class="material-icons font-jumbo s-font-xxxlarge">{{icon}}</i>
            </span>
        </div>
    </div>
{{/cards}}